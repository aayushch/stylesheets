QScrollBar:horizontal {
    border-top: 1px solid #222222;
    background-color: #333333;
    height: 13px;
    margin: 0px 13px 0 13px;
}


QScrollBar::handle:horizontal
{
    background-color: #666666;
    min-height: 20px;
}

QScrollBar::add-line:horizontal {
    border-top: 1px solid #222222;
    border-radius: 0px;
    background-color: #333333;
    width: 13px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    border-top: 1px solid #222222;
    border-radius: 0px;
    background-color: #333333;
    width: 13px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::left-arrow
{
    height: 10px;
    width: 10px;
    image: url(:/scroll_left_arrow.png);
}

QScrollBar::right-arrow
{
    height: 10px;
    width: 10px;
    image: url(:/scroll_right_arrow.png);
}

QScrollBar:vertical
{
    background-color: #333333;
    width: 13px;
    margin: 13px 0 13px 0;
    border-left: 1px solid #222222;
}

QScrollBar::handle:vertical
{
    background-color: #666666;
    min-height: 20px;
    border-radius: 0px;
}

QScrollBar::add-line:vertical
{
    border-left: 1px solid #222222;
    border-radius: 0px;
    background-color: #333333;
    height: 13px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical
{
    border-left: 1px solid #222222;
    border-radius: 0px;
    background-color: #333333;
    height: 13px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow
{
    height: 10px;
    width: 10px;
    image: url(:/scroll_up_arrow.png);
}

QScrollBar::down-arrow
{
    height: 10px;
    width: 10px;
    image: url(:/scroll_down_arrow.png);
}

QSplitter {
    background-color: #2d2d2d;
    width: 6px;
    height: 6px;
}

/* Main horizontal above log pane only*/
QSplitter#centralSplitter::handle {
    height: 6px;
    width: 4px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #333333, stop: 0.17 #333333,
                                      stop: 0.18 #444444, stop: 0.83 #444444,
                                      stop: 0.84 #333333, stop: 1.0 #333333);
}

QSplitter::handle:horizontal {
    height: 4px;
    width: 4px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                      stop: 0 #333333, stop: 0.17 #333333,
                                      stop: 0.18 #444444, stop: 1.0 #444444);
    image: url(:/splitter_handle_horizontal.png);
}

QSplitter::handle:vertical {
    height: 4px;
    width: 4px;
    background-color: #2d2d2d;
    image: url(:/splitter_handle_vertical.png);
}

QSplitter#P4MergeSplitter::handle:vertical {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #333333, stop: 0.17 #333333,
                                      stop: 0.18 transparent, stop: 0.83 transparent,
                                      stop: 0.84 #333333, stop: 1.0 #333333);
}

